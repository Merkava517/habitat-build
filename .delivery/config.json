{
  "version": "2",
  "build_cookbook": {
    "path": ".delivery/build-cookbook",
    "name": "build-cookbook"
  },
  "delivery-truck": {
    "lint": {
      "enable_cookstyle": true,
      "foodcritic": {
        "ignore_rules": ["FC016"],
        "excludes": ["spec", "test"]
      }
    }
  },
  "skip_phases": [],
  "job_dispatch": {
  "version": "v2",
  "filters": {
    "default": {
      "platform": ["ubuntu"],
      "platform_version": ["16.04"]
    }
  }
},
  "dependencies": []
}
